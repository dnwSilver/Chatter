{
  "rules": {
    "semicolon": [
      true,
      "never"
    ],
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "space-before-function-paren": [false, "always"],
    "no-empty-interface": true,
    "whitespace": [false, "check-module"],
    "indent": [true, "spaces", 2]
  }
}
